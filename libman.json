{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "bootstrap@5.0.2",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "dist/js/bootstrap.bundle.min.js",
        "dist/css/bootstrap.min.css"
      ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/jquery/",
      "files": [
        "dist/jquery.min.js"
      ]
    },
    {
      "library": "jquery-validation@1.19.3",
      "destination": "wwwroot/jquery-validation/",
      "files": [
        "dist/jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "bootstrap-filestyle@1.2.1",
      "destination": "wwwroot/lib/bootstrap-filestyle/",
      "files": [
        "src/bootstrap-filestyle.min.js"
      ]
    }
  ]
}